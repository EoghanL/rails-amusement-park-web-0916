<%= form_for @user do |f| %>
  <%= f.label "Name"  %>
  <%= f.text_field :name, id: "user[name]" %>
  <%= f.label "Nausea" %>
  <%= f.text_field :nausea, id: "user[nausea]"  %>
  <%= f.label "Happiness"  %>
  <%= f.text_field :happiness, id: "user[happiness]" %>
  <%= f.label "height"  %>
  <%= f.text_field :height, id: "user[height]" %>
  <%= f.label "tickets"  %>
  <%= f.text_field :tickets, id: "user[tickets]" %>
  <%= f.label "password" %>
  <%= f.password_field :password, id: "user[password]" %>
  <%= f.label "admin"  %><br>
  <%= f.check_box :admin, id: "user[admin]" %>
  <%= f.submit %>
<% end %>
